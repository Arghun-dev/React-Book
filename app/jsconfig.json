{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages"]
    },
    "module": "esnext",
    "allowSyntheticDefaultImports": true,
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react"
  },
  "exclude": ["lib", "node_modules"]
}
